<body>

	<div class="master-container">
		<div class="row-wrap">
			<h2>Current Song</h2>
			<a href="/lookify/dashboard" class="dashboard-link">Dashboard</a> 
		</div>

		<div class="container">
			<h3>
				Title: <c:out value="${Song.title}" />
			</h3>
			<h6>
				Artist: <c:out value="${Song.artist}" />
			</h6>
			<h6>
				Rating: <c:out value="${Song.rating}" />
			</h6>
		</div>
	</div>

</body>
